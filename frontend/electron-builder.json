{
    "appId": "app.netlify.mantraminder",
    "productName": "Mantra minder",
    "directories": {
      "output": "dist"
    },
    "files": ["**/*"],
    "extraResources": [
      "../backend/**/*"
    ],
    "asar": true,
    "win": {
      "target": ["nsis"],
      "icon": "logo.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "logo.icns"
    }
  }